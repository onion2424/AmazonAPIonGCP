{
  "name": "src",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "./src/index.js",
  "scripts": {
    "reportReceive:create": "node ./src/EntryPoint/Build/index.js -reportReceive -release",
    "reportReceive:image": "docker build -t asia-northeast1-docker.pkg.dev/amazon-api-report/amazon-report-repository/report-receive-image .",
    "reportReceive:push": "docker push asia-northeast1-docker.pkg.dev/amazon-api-report/amazon-report-repository/report-receive-image",
    "reportReceive:job": "gcloud run jobs deploy report-receive-run --image asia-northeast1-docker.pkg.dev/amazon-api-report/amazon-report-repository/report-receive-image --region=asia-northeast1 --tasks=1 --cpu=1 --max-retries=0 --memory=512Mi --parallelism=1 --task-timeout=3600 --region asia-northeast1",
    "reportReceive:build": "run-s reportReceive:create reportReceive:image reportReceive:push reportReceive:job",
    "reportReceive:create_test": "node ./src/EntryPoint/Build/index.js -reportReceive",
    "reportReceive:image_test": "docker build -t asia-northeast1-docker.pkg.dev/amazon-api-report/amazon-report-repository/test-report-receive-image .",
    "reportReceive:push_test": "docker push asia-northeast1-docker.pkg.dev/amazon-api-report/amazon-report-repository/test-report-receive-image",
    "reportReceive:job_test": "gcloud run jobs deploy test-report-receive-run --image asia-northeast1-docker.pkg.dev/amazon-api-report/amazon-report-repository/test-report-receive-image --region=asia-northeast1 --tasks=1 --cpu=1 --max-retries=0 --memory=512Mi --parallelism=1 --task-timeout=3600 --region asia-northeast1",
    "reportReceive:build_test": "run-s reportReceive:create_test reportReceive:image_test reportReceive:push_test reportReceive:job_test",
    "reportReceive": "node ./src/EntryPoint/Report/index.js",
    "userSetting:create": "node ./src/EntryPoint/User/build.js -release",
    "userSetting:image": "docker build -t asia-northeast1-docker.pkg.dev/amazon-api-report/amazon-report-repository/user-setting-image .",
    "userSetting:push": "docker push asia-northeast1-docker.pkg.dev/amazon-api-report/amazon-report-repository/user-setting-image",
    "userSetting:job": "gcloud run jobs deploy user-setting-run --image asia-northeast1-docker.pkg.dev/amazon-api-report/amazon-report-repository/user-setting-image --region=asia-northeast1 --tasks=1 --cpu=1 --max-retries=0 --memory=512Mi --parallelism=1 --task-timeout=60 --region asia-northeast1",
    "userSetting:create_test": "node ./src/EntryPoint/User/build.js",
    "userSetting:image_test": "docker build -t asia-northeast1-docker.pkg.dev/amazon-api-report/amazon-report-repository/test-user-setting-image .",
    "userSetting:push_test": "docker push asia-northeast1-docker.pkg.dev/amazon-api-report/amazon-report-repository/test-user-setting-image",
    "userSetting:job_test": "gcloud run jobs deploy test-user-setting-run --image asia-northeast1-docker.pkg.dev/amazon-api-report/amazon-report-repository/test-user-setting-image --region=asia-northeast1 --tasks=1 --cpu=1 --max-retries=0 --memory=512Mi --parallelism=1 --task-timeout=60 --region asia-northeast1",
    "userSetting": "node ./src/EntryPoint/User/index.js",
    "userSetting:build": "run-s userSetting:create userSetting:image userSetting:push userSetting:job",
    "userSetting:build_test": "run-s userSetting:create_test userSetting:image_test userSetting:push_test userSetting:job_test",
    "test:build": "node ./src/EntryPoint/TestRunner/index.js -release",
    "test:build_test": "node ./src/EntryPoint/TestRunner/index.js",
    "test": "node --test",
    "test_only": "node --test --test-only",
    "test:test": "run-s test:build_test test",
    "test:release": "run-s test:build test",
    "test:test_only": "run-s test:build_test test_only",
    "test:release_only": "run-s test:build test_only"
  },
